{
  "name": "prsk-lab",
  "description": "PrskLab",
  "version": "0.1.0",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@hono/swagger-ui": "^0.5.2",
    "@hono/zod-openapi": "^1.1.3",
    "@hookform/resolvers": "^5.2.2",
    "@mui/material": "^7.3.1",
    "@prisma/client": "^6.16.2",
    "@prisma/extension-accelerate": "^2.0.2",
    "@radix-ui/react-label": "^2.1.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "hono": "^4.9.9",
    "jose": "^6.0.13",
    "material-symbols": "^0.34.1",
    "next": "15.5.0",
    "next-auth": "^4.24.11",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "react-hot-toast": "^2.6.0",
    "react-scroll": "^1.9.3",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@tanstack/react-query": "^5.90.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-scroll": "^1.8.10",
    "@vitest/ui": "^4.0.8",
    "axios": "^1.12.2",
    "chalk": "^5.6.2",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-perfectionist": "^4.15.0",
    "fixpack": "^4.0.0",
    "happy-dom": "^20.0.10",
    "husky": "^9.1.7",
    "npm-run-all2": "^8.0.4",
    "orval": "^7.13.0",
    "prettier-plugin-organize-imports": "^4.2.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "prisma": "^6.16.2",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "vitest": "^4.0.8"
  },
  "main": "package.json",
  "private": true,
  "scripts": {
    "build": "run-s build:*",
    "build:api": "orval",
    "build:next": "next build --turbopack",
    "build:prisma": "make prisma-generate",
    "dev": "next dev --turbopack --port 30000",
    "fix": "run-p --continue-on-error fix:*",
    "fix:css": "run-s fix:css:prettier",
    "fix:css:prettier": "prettier './**/*.css' --write",
    "fix:js": "run-s fix:js:eslint fix:js:prettier",
    "fix:js:eslint": "eslint src --fix",
    "fix:js:prettier": "prettier './**/*.{js,jsx,mjs,ts,tsx}' --write",
    "fix:json": "prettier './**/*.json' --write",
    "fix:md": "prettier './**/*.md' --write",
    "fix:pack": "fixpack",
    "generate:api": "orval",
    "generate:openapi": "curl http://localhost:30000/api/openapi.json -o openapi.json",
    "lint": "eslint",
    "postinstall": "prisma generate --no-engine",
    "prepare": "husky",
    "start": "next start",
    "test": "vitest",
    "test:all": "vitest run --reporter=verbose",
    "test:check-coverage": "ts-node scripts/check-handler-tests.ts",
    "test:integration": "vitest run --reporter=verbose src/__tests__/integration",
    "test:run": "vitest run --reporter=verbose",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run --reporter=verbose --exclude=**/__tests__/integration/**"
  }
}
